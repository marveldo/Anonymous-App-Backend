{
  "name": "expressfirstone",
  "version": "1.0.0",
  "description": "Express With typescript",
  "main": "dist/app.ts",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,json --exec ts-node src/app.ts",
    "build": "pnpm prisma:generate && pnpm tsc && pnpm copyfiles --up 1 \"src/generated/**/*\" dist",
    "prisma:generate": "prisma generate",
    "start": "node dist/app.js",
    "clean": "rm -rf dist",
    "prestart": "pnpm run build",
    "lint": "eslint src/**/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "cors": "^2.8.5",
    "eslint": "^9.35.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.18.0",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "swagger-autogen": "^2.23.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
